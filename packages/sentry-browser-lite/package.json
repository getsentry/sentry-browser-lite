{
  "name": "sentry-browser-lite",
  "version": "0.1.0",
  "description": "A lightweight JavaScript SDK for Browser environments",
  "homepage": "https://github.com/getsentry/sentry-browser-lite#readme",
  "bugs": {
    "url": "https://github.com/getsentry/sentry-browser-lite/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/getsentry/sentry-browser-lite.git"
  },
  "author": "Sentry",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "main": "dist/index.js",
  "module": "dist/sentry-browser-lite.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@playwright/test": "^1.13.1",
    "@size-limit/preset-small-lib": "^5.0.2",
    "agadoo": "^2.0.0",
    "husky": "^7.0.1",
    "size-limit": "^5.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prepublishOnly": "agadoo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/sentry-browser-lite.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sentry-browser-lite.esm.js",
      "limit": "10 KB"
    }
  ]
}
